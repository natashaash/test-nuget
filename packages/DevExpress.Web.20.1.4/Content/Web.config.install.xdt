<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections xdt:Transform="InsertBefore(/configuration/*[1])">
  </configSections>
  <configSections xdt:Locator="XPath(/configuration/configSections[last()])">
    <sectionGroup xdt:Transform="Remove" xdt:Locator="Condition(@name='devExpress')" />
    <sectionGroup name="devExpress" xdt:Transform="Insert" >
      <section name="themes" type="DevExpress.Web.ThemesConfigurationSection, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" requirePermission="false" />
      <section name="compression" type="DevExpress.Web.CompressionConfigurationSection, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" requirePermission="false" />
      <section name="settings" type="DevExpress.Web.SettingsConfigurationSection, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" requirePermission="false" />
      <section name="errors" type="DevExpress.Web.ErrorsConfigurationSection, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" requirePermission="false" />
	  <section name="resources" type="DevExpress.Web.ResourcesConfigurationSection, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <configSections xdt:Transform="RemoveAll" xdt:Locator="Condition(count(*)=0)" />

  <system.web>
    <httpModules xdt:Transform="InsertIfMissing">
    </httpModules>
    <httpHandlers  xdt:Transform="InsertIfMissing">
    </httpHandlers>
	<pages xdt:Transform="InsertIfMissing">
    </pages>
  </system.web>
  <system.web>
    <httpModules>
      <add xdt:Transform="Remove" xdt:Locator="Condition(@name='ASPxHttpHandlerModule')" />
      <add type="DevExpress.Web.ASPxHttpHandlerModule, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" name="ASPxHttpHandlerModule"
           xdt:Transform="Insert" />
    </httpModules>
    <httpHandlers>
      <add xdt:Transform="Remove" xdt:Locator="Condition(@path='DX.ashx')" />
      <add type="DevExpress.Web.ASPxHttpHandlerModule, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" verb="GET,POST" path="DX.ashx" validate="false" xdt:Transform="Insert" />
      <add xdt:Transform="Remove" xdt:Locator="Condition(@path='ASPxUploadProgressHandlerPage.ashx')" />
      <add type="DevExpress.Web.ASPxUploadProgressHttpHandler, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" verb="GET,POST" path="ASPxUploadProgressHandlerPage.ashx" validate="false"
           xdt:Transform="Insert" />
    </httpHandlers>
	<pages>
      <controls xdt:Transform="InsertIfMissing">
      </controls>
    </pages>
  </system.web>
  <system.web>
    <pages>
      <controls>
		<add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Data.Linq')" />
		<add tagPrefix="dx" namespace="DevExpress.Data.Linq" assembly="DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a"/>
        <add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Web')" />
        <add xdt:Transform="Insert" tagPrefix="dx" namespace="DevExpress.Web" assembly="DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" />
		<add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Web.ASPxTreeList')" />
		<add xdt:Transform="Insert" tagPrefix="dx" namespace="DevExpress.Web.ASPxTreeList" assembly="DevExpress.Web.ASPxTreeList.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" />
		<add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Web.ASPxPivotGrid')" />
		<add xdt:Transform="Insert" tagPrefix="dx" namespace="DevExpress.Web.ASPxPivotGrid" assembly="DevExpress.Web.ASPxPivotGrid.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" />
		<add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Web.ASPxSpellChecker')" />
		<add xdt:Transform="Insert" tagPrefix="dx" namespace="DevExpress.Web.ASPxSpellChecker" assembly="DevExpress.Web.ASPxSpellChecker.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" />
		<add xdt:Transform="Remove" xdt:Locator="Condition(@namespace='DevExpress.Web.ASPxHtmlEditor')" />
		<add xdt:Transform="Insert" tagPrefix="dx" namespace="DevExpress.Web.ASPxHtmlEditor" assembly="DevExpress.Web.ASPxHtmlEditor.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" />
      </controls>
    </pages>
  </system.web>

  <system.webServer xdt:Transform="InsertIfMissing">
  </system.webServer>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" xdt:Transform="InsertIfMissing">
    </modules>
    <handlers xdt:Transform="InsertIfMissing">
    </handlers>
  </system.webServer>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" xdt:Transform="InsertIfMissing" />
    <modules runAllManagedModulesForAllRequests="true" xdt:Transform="InsertIfMissing">
      <add xdt:Transform="Remove" xdt:Locator="Condition(@name='ASPxHttpHandlerModule')"/>
      <add type="DevExpress.Web.ASPxHttpHandlerModule, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" name="ASPxHttpHandlerModule"
           xdt:Transform="Insert" />
    </modules>
    <handlers>
      <add xdt:Transform="Remove" xdt:Locator="Condition(@name='ASPxHttpHandlerModule')" />
      <add type="DevExpress.Web.ASPxHttpHandlerModule, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" verb="GET,POST" path="DX.ashx" name="ASPxHttpHandlerModule" preCondition="integratedMode" xdt:Transform="Insert" />
      <add xdt:Transform="Remove" xdt:Locator="Condition(@name='ASPxUploadProgressHandler')" />
      <add type="DevExpress.Web.ASPxUploadProgressHttpHandler, DevExpress.Web.v20.1, Version=20.1.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" verb="GET,POST" path="ASPxUploadProgressHandlerPage.ashx" name="ASPxUploadProgressHandler" preCondition="integratedMode"
           xdt:Transform="Insert" />
    </handlers>
  </system.webServer>

  <devExpress xdt:Transform="InsertIfMissing">
    <themes enableThemesAssembly="true" styleSheetTheme="" theme="" customThemeAssemblies="" />
    <compression enableHtmlCompression="false" enableCallbackCompression="true" enableResourceCompression="true" enableResourceMerging="true" />
    <settings doctypeMode="Html5" rightToLeft="false" ieCompatibilityVersion="edge" />
    <errors callbackErrorRedirectUrl="" />
  </devExpress>
</configuration>